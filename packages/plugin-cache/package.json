{
  "name": "@tablecraft/plugin-cache",
  "version": "0.1.0-beta.5",
  "description": "Caching plugin for TableCraft â€” memory, Redis, or bring your own",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./redis": {
      "types": "./dist/providers/redis.d.ts",
      "import": "./dist/providers/redis.js"
    },
    "./upstash": {
      "types": "./dist/providers/upstash.d.ts",
      "import": "./dist/providers/upstash.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun run --bun tsc",
    "dev": "bun run --bun tsc --watch",
    "test": "vitest",
    "typecheck": "bun run --bun tsc --noEmit"
  },
  "peerDependencies": {
    "@tablecraft/engine": "^0.1.0-beta.3"
  },
  "peerDependenciesMeta": {
    "ioredis": { "optional": true },
    "@upstash/redis": { "optional": true }
  },
  "devDependencies": {
    "@tablecraft/engine": "workspace:*",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "tablecraft",
    "cache",
    "redis",
    "upstash",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacksonkasi1/TableCraft.git",
    "directory": "packages/plugin-cache"
  },
  "homepage": "https://github.com/jacksonkasi1/TableCraft#readme",
  "bugs": {
    "url": "https://github.com/jacksonkasi1/TableCraft/issues"
  },
  "license": "MIT"
}
