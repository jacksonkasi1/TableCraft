{
  "name": "@tablecraft/adapter-next",
  "version": "0.1.1",
  "description": "Next.js App Router adapter for TableCraft",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun run --bun tsc",
    "dev": "bun run --bun tsc --watch",
    "test": "vitest",
    "typecheck": "bun run --bun tsc --noEmit"
  },
  "peerDependencies": {
    "@tablecraft/engine": ">=0.1.0",
    "next": ">=14.0.0"
  },
  "devDependencies": {
    "@tablecraft/engine": "workspace:*",
    "@types/node": "^25.2.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "tablecraft",
    "nextjs",
    "adapter",
    "next"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacksonkasi1/TableCraft.git",
    "directory": "packages/adapter-next"
  },
  "homepage": "https://github.com/jacksonkasi1/TableCraft#readme",
  "bugs": {
    "url": "https://github.com/jacksonkasi1/TableCraft/issues"
  },
  "license": "MIT"
}
