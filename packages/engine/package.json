{
  "name": "@tablecraft/engine",
  "version": "0.1.2",
  "description": "The backend query engine for TableCraft",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
