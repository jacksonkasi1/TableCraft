{
  "name": "@tablecraft/codegen",
  "version": "0.0.1",
  "description": "Generate TypeScript types from TableCraft API metadata",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tablecraft-codegen": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --dts",
    "typecheck": "tsc --noEmit",
    "dev": "tsup src/index.ts src/cli.ts --format esm --dts --watch"
  },
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.32",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "tablecraft",
    "codegen",
    "typescript",
    "types",
    "api"
  ],
  "license": "MIT"
}