{
  "name": "@tablecraft/adapter-express",
  "version": "0.1.0-beta.3",
  "description": "Express adapter for TableCraft",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
